<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chromeinfotech.jsonparsing">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.chromeinfotech.Ui.MainActivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.chromeinfotech.Ui.Xmlparsing.MyXmlParserActivity" />
        <activity android:name="com.chromeinfotech.Ui.Xmlparsing.Xmlparcerwitharraylist.XmlParcerArraylist" />
        <activity android:name="com.chromeinfotech.Ui.JsonparsingwithListview.JsonparcerActivityListview" />
        <activity android:name="com.chromeinfotech.Ui.JsonparsingwithExpandablelistview.JsonParcerActivity" />
        <activity android:name="com.chromeinfotech.Ui.Jsonparsingfromurl.JsonParsingfromUrl"></activity>
    </application>

</manifest>